{
    "name": "orchestra/asset",
    "description": "Asset Component for Orchestra Platform",
    "homepage": "http://orchestraplatform.com/docs/latest/components/asset/",
    "keywords": ["orchestra-platform", "orchestral", "laravel", "asset"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Asset\\" : "src/"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "orchestra/html": "~3.1.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "illuminate/filesystem": "~5.1.0"
    },
    "suggest": {
        "laravel/framework": "Allow using orchestra/asset component outside of Orchestra Platform (~5.1).",
        "orchestra/foundation": "Allow using orchestra/asset with Orchestra Platform (~3.1).",
        "illuminate/filesystem": "Allow using orchestra/asset component outside of Laravel app (~5.1)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    },
    "minimum-stability": "dev"
}
