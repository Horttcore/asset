{
    "name": "orchestra/asset",
    "description": "Asset Component for Orchestra Platform",
    "homepage": "http://orchestraplatform.com/docs/latest/components/asset/",
    "keywords": ["orchestra-platform", "orchestral", "laravel", "asset"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Asset\\" : "src/"
        }
    },
    "require": {
        "php": ">=5.6.0",
        "laravie/html": "~5.4.0",
        "orchestra/contracts": "~3.4.0",
        "orchestra/support-facades": "~3.4.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.4",
        "illuminate/filesystem": "~5.3.0"
    },
    "suggest": {
        "laravel/framework": "Allow using orchestra/asset component outside of Orchestra Platform (~5.4).",
        "orchestra/html": "Allow using orchestra/asset with HTML component (~3.4).",
        "orchestra/foundation": "Allow using orchestra/asset with Orchestra Platform (~3.4).",
        "illuminate/filesystem": "Allow using orchestra/asset component outside of Laravel app (~5.4)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.4-dev"
        }
    },
    "minimum-stability": "dev"
}
